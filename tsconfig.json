{
  "compilerOptions": {
    "target": "esnext", // Cambia a esnext o es2020
    "module": "esnext", // Cambia a esnext
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.astro"],
  "exclude": ["node_modules"]
}
